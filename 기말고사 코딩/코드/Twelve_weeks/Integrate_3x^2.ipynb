    "import numpy\n",
    "\n",
    "def calc(x):\n",
    "    return 3 * x ** 2;\n",
    "\n",
    "def Operator(n, m):\n",
    "    tmp = 0.0\n",
    "    tmp1 = 0.0\n",
    "    for n in numpy.arange(n, m, 0.5):\n",
    "        tmp = calc(n + 0.5)\n",
    "        tmp1 += 0.5 * tmp\n",
    "    return tmp1;\n",
    "\n",
    "    \n",
    "result = Operator(0.0, 1.0)\n",
    "\n",
    "print(result)"
   ]
