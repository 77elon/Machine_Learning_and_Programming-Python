    "import numpy\n",
    "import random\n",
    "#plot?\n",
    "\n",
    "n=5000\n",
    "count=0.0\n",
    "for i in range(n):\n",
    "  x=random.random()\n",
    "  y=random.random()\n",
    "  if(x*x+y*y<1):\n",
    "    count+=1\n",
    "result=4*count/n\n",
    "\n",
    "print(result)"
   ]
  }
